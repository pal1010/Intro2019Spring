 */

import java.util.Scanner; //required import for terminal IO
//This will give us "tools" from a prewritten class to use in this code.
public class Greetings
{
    public static void main(String args[])
    {
        Scanner in = new Scanner(System.in); //Required Line for terminal IO
        System.out.println("Hello");
        System.out.print("Please enter a phrase :");//Your Name 
        System.out.println("today is");
        System.out.print("Please enter a phrase :");//Day of week
        System.out.println("and the date is");
        System.out.print("Please enter a phrase :");//Todays date
        
    }
}
